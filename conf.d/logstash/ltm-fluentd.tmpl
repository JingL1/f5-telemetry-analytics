{
  "index_patterns": [
    "ltm-fluentd-*"
  ],
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 0
  },
  "mappings": {
    "properties": {
      "timestamp": {
        "type": "date"
      },
      "client-ip": {
        "type": "ip"
      },
      "server-ip": {
        "type": "ip"
      },
      "geo": {
        "type": "geo_point"
      },
      "latency": {
        "type": "integer"
      }
    }
  }
}